
@{
    Layout = "_Layout";
}

<!-- NAV-FIRST -->
<div class="nav-begin justify-content-between">
    <h2 class="title-main">MEN</h2>
    <a href="#">Shop women</a>
</div>

<!-- SLIDER ADS -->
<div>
    <div class="uk-panel uk-padding uk-padding-remove-horizontal uk-padding-remove-top">
        <img src="https://a.storyblok.com/f/177537/2619x700/6af66def5d/m-sale.png" width="1800" height="1200" alt="">
        <div class="uk-position-center uk-text-center">
            <p uk-slider-parallax="x: 200,-200">Đồ S2HAND chất lượng cao, giá hợp lý.</p>
            <button class="uk-button uk-box-shadow-hover-xlarge uk-button-color"><a asp-controller="Filter" asp-action="Index" style="color: #fff;">Shop Now</a><span uk-icon="arrow-right"></span></button>
        </div>
    </div>
    <div class="uk-position-relative uk-visible-toggle uk-light" tabindex="-1" uk-slider>

        <ul class="uk-slider-items uk-child-width-1-2 uk-child-width-1-3@m uk-grid">
            <li>
                <div class="uk-panel">
                    <img src="https://a.storyblok.com/f/177537/900x600/10a6b3612e/45.png/m/1080x0" width="400" height="600" alt="">
                </div>
            </li>
            <li>
                <div class="uk-panel">
                    <img src="https://a.storyblok.com/f/177537/900x600/62c6deaeca/product-edit-3-2-900x600-2.png/m/1080x0" width="400" height="600" alt="">
                </div>
            </li>
            <li>
                <div class="uk-panel">
                    <img src="https://a.storyblok.com/f/177537/900x600/ea9896a1cc/46.png/m/1080x0" width="400" height="600" alt="">
                </div>
            </li>
        </ul>

        <a class="uk-position-center-left uk-position-small uk-hidden-hover" href="#" uk-slidenav-previous uk-slider-item="previous"></a>
        <a class="uk-position-center-right uk-position-small uk-hidden-hover" href="#" uk-slidenav-next uk-slider-item="next"></a>

    </div>
</div>

<!-- list cart -->

<div class="uk-margin-medium-top">
    <h3>Danh Mục Sản Phẩm</h3>
    <!-- card category -->
    <ul class="uk-child-width-1-3 uk-child-width-1-6@m uk-background-cate uk-padding" uk-grid>
    @await Component.InvokeAsync("Category")
    </ul>
</div>

